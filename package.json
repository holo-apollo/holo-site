  "scripts": {
    "dev": "node server",
    "start": "NODE_ENV=production node server",
    "build": "next build",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "test:coverage": "yarn test --coverage",
    "eslint": "eslint src",
    "eslint:tap": "eslint src -f tap",
    "flow": "flow",
    "flow:stop": "flow stop",
    "storybook": "start-storybook -p 9001 -c storybook/config -s static",
